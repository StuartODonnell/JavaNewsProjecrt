<p class="pageHeader">All Articles</p>

<div class="container">
    <table>
        <tr>
            <th>Heading</th>
            <th>Sub Heading</th>
            <th>Details</th>
        </tr>
        #foreach($article in $articles)
        <tr>
            <td>$article.getHeading()</td>
            <td>$article.getSubHeading()</td>
            <td><a href ="/articles/$article.getId()" class="linktobody" >Full Story</a></td>
        <tr>
            #end
    </table>
</div>