<p class="newsPageTitle">Search results</p>

<div>
    #foreach($article in $foundArticles)
    <div class="floating-box">
        <div class="heading">$article.getHeading()</div>
        <div class="subheading">$article.getSubHeading()</div>
        <p class="articleDate">$article.returnCalenderasString()</p>
        <a href ="/news/$article.getId()" class="linktobody" >Full Story</a>
    </div>
    #end
</div>