<p class="pageHeader">All Articles</p>

<div class="container">
    <table>

        <tr>
            <th>Name</th>
            <th>Articles</th>
            <th>Details</th>
        </tr>
        #foreach($journalist in $journalists)
        <tr>
            <td>$journalist.getName()</td>
            <td>$journalist.getArticles</td>
            <td><a href="/journalists/$journalist.getId()">Details</a></td>
        <tr>
            #end
    </table>
</div>